{
  "name": "bunsen-tools",
  "version": "1.2.0",
  "description": "convert ui-schema to ui-schema-2",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/cli.js",
    "watch": "npm-watch",
    "build-lib": "babel src -d lib --presets es2015 --plugins add-module-exports",
    "build-cli": "babel cli -d bin --presets es2015 --plugins add-module-exports",
    "build": "npm run build-lib && npm run build-cli",
    "lint": "eslint *.js src bin tests",
    "test": "npm run lint && npm run utest",
    "utest": "npm run build && istanbul cover _mocha -- tests"
  },
  "watch": {
    "utest": {
      "patterns": [
        "src",
        "tests",
        "cli"
      ],
      "extentions": "js,json",
      "quiet": true
    }
  },
  "author": "Chris Stoll",
  "license": "MIT",
  "dependencies": {
    "bunsen-core": "^0.7.2",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "fs-promise": "^0.5.0",
    "promise": "^7.1.1",
    "z-schema": "^3.17.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-watch": "^2.0.2",
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "child-process-promise": "^2.0.3",
    "eslint": "^3.1.1",
    "eslint-config-frost-standard": "^3.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "npm-watch": "^0.1.5",
    "sinon": "^1.17.4"
  },
  "bin": {
    "bunsen": "bin/index.js"
  }
}